        </ns3:generalInformation>
        <ns3:conditionAndStandard>
            <standard>&lt;p&gt;&lt;span id=&quot;jS$72&quot; class=&quot;j2&quot;&gt;Extract&lt;/span&gt; &lt;span id=&quot;jS$73&quot; class=&quot;j2&quot;&gt;information&lt;/span&gt; &lt;span id=&quot;jS$74&quot; class=&quot;j2&quot;&gt;from&lt;/span&gt;&amp;nbsp;&lt;span id=&quot;jS$75&quot; class=&quot;j2&quot;&gt;an&lt;/span&gt;&amp;nbsp;&lt;span id=&quot;jS$76&quot; class=&quot;j4&quot;&gt;ACO&lt;/span&gt; &lt;span id=&quot;jS$77&quot; class=&quot;j2&quot;&gt;and&lt;/span&gt;&amp;nbsp;&lt;span id=&quot;jS$78&quot; class=&quot;j2&quot;&gt;a&lt;/span&gt; &lt;span id=&quot;jS$79&quot; class=&quot;j4&quot;&gt;SPINS&lt;/span&gt; &lt;span id=&quot;jS$80&quot; class=&quot;j2&quot;&gt;then&lt;/span&gt; &lt;span id=&quot;jS$81&quot; class=&quot;j2&quot;&gt;post&lt;/span&gt; &lt;span id=&quot;jS$82&quot; class=&quot;j2&quot;&gt;in&lt;/span&gt; &lt;span id=&quot;jS$83&quot; class=&quot;j2&quot;&gt;accordance&lt;/span&gt; &lt;span id=&quot;jS$84&quot; class=&quot;j2&quot;&gt;with&lt;/span&gt; &lt;span id=&quot;jS$85&quot; class=&quot;j2&quot;&gt;the&lt;/span&gt; &lt;span id=&quot;jS$86&quot; class=&quot;j2&quot;&gt;Tactical&lt;/span&gt; &lt;span id=&quot;jS$87&quot; class=&quot;j2&quot;&gt;Level&lt;/span&gt; &lt;span id=&quot;jS$90&quot; class=&quot;j4&quot;&gt;SOP&lt;/span&gt;.&lt;/p&gt;</standard>
            <condition>&lt;p&gt;&lt;span id=&quot;jS$6&quot; class=&quot;j1&quot;&gt;As&lt;/span&gt; &lt;span id=&quot;jS$7&quot; class=&quot;j2&quot;&gt;an&lt;/span&gt;&amp;nbsp;&lt;span id=&quot;jS$8&quot; class=&quot;j1&quot;&gt;Aviation&lt;/span&gt; &lt;span id=&quot;jS$9&quot; class=&quot;j1&quot;&gt;Soldier&lt;/span&gt; &lt;span id=&quot;jS$10&quot; class=&quot;j1&quot;&gt;assigned&lt;/span&gt; &lt;span id=&quot;jS$11&quot; class=&quot;j1&quot;&gt;to&lt;/span&gt; &lt;span id=&quot;jS$12&quot; class=&quot;j1&quot;&gt;a&lt;/span&gt; &lt;span id=&quot;jS$13&quot; class=&quot;j1&quot;&gt;Tactical&lt;/span&gt; &lt;span id=&quot;jS$14&quot; class=&quot;j1&quot;&gt;Operations&lt;/span&gt; &lt;span id=&quot;jS$15&quot; class=&quot;j1&quot;&gt;Center&lt;/span&gt; (&lt;span id=&quot;jS$16&quot; class=&quot;j1&quot;&gt;TOC&lt;/span&gt;) &lt;span id=&quot;jS$17&quot; class=&quot;j1&quot;&gt;with&lt;/span&gt; &lt;span id=&quot;jS$18&quot; class=&quot;j1&quot;&gt;the&lt;/span&gt; &lt;span id=&quot;jS$19&quot; class=&quot;j1&quot;&gt;responsibility&lt;/span&gt; &lt;span id=&quot;jS$20&quot; class=&quot;j1&quot;&gt;to&lt;/span&gt; &lt;span id=&quot;jS$21&quot; class=&quot;j1&quot;&gt;identify&lt;/span&gt; &lt;span id=&quot;jS$22&quot; class=&quot;j1&quot;&gt;information&lt;/span&gt; &lt;span id=&quot;jS$23&quot; class=&quot;j1&quot;&gt;from&lt;/span&gt; &lt;span id=&quot;jS$24&quot; class=&quot;j1&quot;&gt;Airspace&lt;/span&gt; &lt;span id=&quot;jS$25&quot; class=&quot;j1&quot;&gt;Command&lt;/span&gt; &lt;span id=&quot;jS$26&quot; class=&quot;j1&quot;&gt;and&lt;/span&gt; &lt;span id=&quot;jS$27&quot; class=&quot;j1&quot;&gt;Control&lt;/span&gt; &lt;span id=&quot;jS$28&quot; class=&quot;j1&quot;&gt;Documents&lt;/span&gt; &lt;span id=&quot;jS$29&quot; class=&quot;j1&quot;&gt;in&lt;/span&gt; &lt;span id=&quot;jS$30&quot; class=&quot;j1&quot;&gt;support&lt;/span&gt; &lt;span id=&quot;jS$31&quot; class=&quot;j1&quot;&gt;of&lt;/span&gt; &lt;span id=&quot;jS$32&quot; class=&quot;j1&quot;&gt;missions&lt;/span&gt; &lt;span id=&quot;jS$33&quot; class=&quot;j2&quot;&gt;given&lt;/span&gt; &lt;span id=&quot;jS$34&quot; class=&quot;j2&quot;&gt;current&lt;/span&gt;&amp;nbsp;&lt;span id=&quot;jS$39&quot; class=&quot;j2&quot;&gt;Airspace&lt;/span&gt; &lt;span id=&quot;jS$40&quot; class=&quot;j2&quot;&gt;Control&lt;/span&gt; &lt;span id=&quot;jS$41&quot; class=&quot;j2&quot;&gt;Orders&lt;/span&gt; &lt;span id=&quot;jS$42&quot; class=&quot;j2&quot;&gt;and&lt;/span&gt; &lt;span id=&quot;jS$43&quot; class=&quot;j4&quot;&gt;SPINS&lt;/span&gt;. &lt;span id=&quot;jS$44&quot; class=&quot;j2&quot;&gt;You&lt;/span&gt; &lt;span id=&quot;jS$45&quot; class=&quot;j2&quot;&gt;have&lt;/span&gt; &lt;span id=&quot;jS$46&quot; class=&quot;j2&quot;&gt;access&lt;/span&gt; &lt;span id=&quot;jS$47&quot; class=&quot;j2&quot;&gt;to&lt;/span&gt; &lt;span id=&quot;jS$48&quot; class=&quot;j4&quot;&gt;FM&lt;/span&gt; &lt;span id=&quot;jS$49&quot; class=&quot;j4&quot;&gt;3&lt;/span&gt;-&lt;span id=&quot;jS$50&quot; class=&quot;j4&quot;&gt;52&lt;/span&gt; (&lt;span id=&quot;jS$51&quot; class=&quot;j2&quot;&gt;Army&lt;/span&gt; &lt;span id=&quot;jS$52&quot; class=&quot;j2&quot;&gt;Airspace&lt;/span&gt; &lt;span id=&quot;jS$53&quot; class=&quot;j2&quot;&gt;Command&lt;/span&gt; &lt;span id=&quot;jS$54&quot; class=&quot;j2&quot;&gt;and&lt;/span&gt; &lt;span id=&quot;jS$55&quot; class=&quot;j2&quot;&gt;Control&lt;/span&gt; &lt;span id=&quot;jS$56&quot; class=&quot;j2&quot;&gt;in&lt;/span&gt; &lt;span id=&quot;jS$57&quot; class=&quot;j2&quot;&gt;a&lt;/span&gt; &lt;span id=&quot;jS$58&quot; class=&quot;j2&quot;&gt;Combat&lt;/span&gt; &lt;span id=&quot;jS$59&quot; class=&quot;j2&quot;&gt;Zone&lt;/span&gt;), &lt;span id=&quot;jS$60&quot; class=&quot;j4&quot;&gt;JP&lt;/span&gt; &lt;span id=&quot;jS$61&quot; class=&quot;j4&quot;&gt;3&lt;/span&gt;-&lt;span id=&quot;jS$62&quot; class=&quot;j4&quot;&gt;52&lt;/span&gt; (&lt;span id=&quot;jS$63&quot; class=&quot;j1&quot;&gt;Joint&lt;/span&gt; &lt;span id=&quot;jS$64&quot; class=&quot;j2&quot;&gt;Airspace&lt;/span&gt; &lt;span id=&quot;jS$63&quot; class=&quot;j1&quot;&gt;Control&lt;/span&gt;),&amp;nbsp;&lt;span id=&quot;jS$65&quot; class=&quot;j2&quot;&gt;and&lt;/span&gt; &lt;span id=&quot;jS$66&quot; class=&quot;j2&quot;&gt;the&lt;/span&gt; &lt;span id=&quot;jS$67&quot; class=&quot;j2&quot;&gt;Tactical&lt;/span&gt; &lt;span id=&quot;jS$68&quot; class=&quot;j2&quot;&gt;Level&lt;/span&gt; &lt;span id=&quot;jS$69&quot; class=&quot;j4&quot;&gt;SOP&lt;/span&gt;.&lt;/p&gt;</condition>
            <specialConditions/>
        </ns3:conditionAndStandard>
        <ns3:taskSteps isParentOrChild="Parent">
            <stepLevel>1</stepLevel>
            <stepDisplayId>1.;</stepDisplayId>
            <sortOrder>1</sortOrder>
            <stepTitle>Extract critical information from the ACO.</stepTitle>
            <statements/>
            <childSteps isParentOrChild="Child">
                <stepLevel>2</stepLevel>
                <stepDisplayId>1.;a.;</stepDisplayId>
                <sortOrder>2</sortOrder>
                <stepTitle>Decode the use of a double slash (//) or a slash-hyphen-double slash (/-//) at the beginning of a line.  </stepTitle>
                <statements>
                    <note>These items indicate the end of an entry or a new section of data.</note>
                </statements>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>2</stepLevel>
                <stepDisplayId>1.;b.;</stepDisplayId>
                <sortOrder>3</sortOrder>
                <stepTitle>Decode the ACO header.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>2</stepLevel>
                <stepDisplayId>1.;c.;</stepDisplayId>
                <sortOrder>4</sortOrder>
                <stepTitle>Decode the first dataset of the header to identify the type of message being received.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;c.;(1);</stepDisplayId>
                <sortOrder>5</sortOrder>
                <stepTitle>The dataset identifier will be either OPER or EXER indicating the reason for the message, followed by a &quot;/&quot;.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;c.;(2);</stepDisplayId>
                <sortOrder>6</sortOrder>
                <stepTitle>The second element in this dataset is the nickname of the exercise or operation codeword itself; &quot;COBRA GOLD&quot;.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;c.;(3);</stepDisplayId>
                <sortOrder>7</sortOrder>
                <stepTitle>For an EXER there may be one additional data block to display any additional identifiers for the exercise, and then a double slash (//) to end the dataset, as in the example below.&#xD;
&#xD;
EXER/OFF 450 FTU TRAINING/-//</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;c.;(4);</stepDisplayId>
                <sortOrder>8</sortOrder>
                <stepTitle>For an OPER message there may be as many as three additional data blocks.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>2</stepLevel>
                <stepDisplayId>1.;d.;</stepDisplayId>
                <sortOrder>9</sortOrder>
                <stepTitle>Decode the second dataset in the header. It contains the Message Identification (MSGID).</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;d.;(1);</stepDisplayId>
                <sortOrder>10</sortOrder>
                <stepTitle>Field 1 is the Message Text Format Identifier. </stepTitle>
                <statements>
                    <note>This field must be populated as &quot;ACO&quot; when it is an ACO message.</note>
                </statements>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;d.;(2);</stepDisplayId>
                <sortOrder>11</sortOrder>
                <stepTitle>Field 2 is the Originator or Issuing Agency.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;d.;(3);</stepDisplayId>
                <sortOrder>12</sortOrder>
                <stepTitle>Field 3 is the Message serial number. </stepTitle>
                <statements>
                    <note>If the ACO is numbered it will be indicated here.</note>
                </statements>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;d.;(4);</stepDisplayId>
                <sortOrder>13</sortOrder>
                <stepTitle>Field 4 is the Month name. </stepTitle>
                <statements>
                    <note>The presence of a month name does NOT by itself imply that the ATO is valid for the entire month.</note>
                </statements>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;d.;(5);</stepDisplayId>
                <sortOrder>14</sortOrder>
                <stepTitle>Field 5 is the Qualifier.  </stepTitle>
                <statements>
                    <note>If this is a change it will indicate &quot;CHG&quot; here.</note>
                </statements>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;d.;(6);</stepDisplayId>
                <sortOrder>15</sortOrder>
                <stepTitle>Field 6 is for the Serial number of the qualifier.  </stepTitle>
                <statements>
                    <note>This indicates which change number it is.  Much of the information in these blocks may be &quot;-&quot; indicating a &quot;no data&quot; entry.</note>
                </statements>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>2</stepLevel>
                <stepDisplayId>1.;e.;</stepDisplayId>
                <sortOrder>16</sortOrder>
                <stepTitle>Decode reference (REF) dataset.</stepTitle>
                <statements>
                    <note>This is optional.</note>
                </statements>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>2</stepLevel>
                <stepDisplayId>1.;f.;</stepDisplayId>
                <sortOrder>17</sortOrder>
                <stepTitle>Decode the ACO Identification, area, and version&#xD;
</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;f.;(1);</stepDisplayId>
                <sortOrder>18</sortOrder>
                <stepTitle>The first element is the dataset identifier &quot;ACOID.&quot;</stepTitle>
                <statements>
                    <note>This dataset is MANDATORY if the message identification is &quot;ACO.&quot;</note>
                </statements>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;f.;(2);</stepDisplayId>
                <sortOrder>19</sortOrder>
                <stepTitle>The second element is the Area of Validity.  </stepTitle>
                <statements>
                    <note>This area is optional and can indicate a geographical area or a command over which the ACO is valid.</note>
                </statements>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>1.;f.;(3);</stepDisplayId>
                <sortOrder>20</sortOrder>
                <stepTitle>The third element is the ACO Serial Number.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>2</stepLevel>
                <stepDisplayId>1.;g.;</stepDisplayId>
                <sortOrder>21</sortOrder>
                <stepTitle>Decode the Geodetic Datum reference.</stepTitle>
                <statements>
                    <note>The geospatial datum is a critical field which ensures everyone is using the same map reference and coordinate systems.&#xD;
</note>
                </statements>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>2</stepLevel>
                <stepDisplayId>1.;h.;</stepDisplayId>
                <sortOrder>22</sortOrder>
                <stepTitle>Decode the next dataset which defines the period covered by the ACO.&#xD;
</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>2</stepLevel>
                <stepDisplayId>1.;i.;</stepDisplayId>
                <sortOrder>23</sortOrder>
                <stepTitle>Decode the final dataset in the ACO header which is normally reserved for contact information.&#xD;
</stepTitle>
                <statements>
                    <note> </note>
                </statements>
            </childSteps>
        </ns3:taskSteps>
        <ns3:taskSteps isParentOrChild="Parent">
            <stepLevel>1</stepLevel>
            <stepDisplayId>2.;</stepDisplayId>
            <sortOrder>24</sortOrder>
            <stepTitle>Decode the approved Airspace Coordinating Measures (ACMs) in the ACO.</stepTitle>
            <statements/>
            <childSteps isParentOrChild="Child">
                <stepLevel>2</stepLevel>
                <stepDisplayId>2.;a.;</stepDisplayId>
                <sortOrder>25</sortOrder>
                <stepTitle>Decode ACMID.  The Airspace Control Means Identification (ACMID) dataset identifies and defines the type and usage of the airspace coordinating measure, as in the following lines.&#xD;
&#xD;
ACMID/ACM:CORRTE/NAME:BUICK/CORRIDOR/USE:AIRCOR//&#xD;
ACMID/OTR:RFHHLL/DESIG:C34/CORRIDOR/OTR:SPAA// </stepTitle>
                <statements>
                    <note> </note>
                </statements>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>2.;a.;(1);</stepDisplayId>
                <sortOrder>26</sortOrder>
                <stepTitle>The first block normally starts out with the field descriptor &quot;ACM:&quot; prior to type of ACM being identified.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>2.;a.;(2);</stepDisplayId>
                <sortOrder>27</sortOrder>
                <stepTitle>The second block in the ACMID dataset is reserved for the ACM Identifier or Name, as in the examples below.&#xD;
                                                        &#xD;
                                                                /NAME:BUICK/&#xD;
                                                                /DESIG:C34//</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>2.;a.;(3);</stepDisplayId>
                <sortOrder>28</sortOrder>
                <stepTitle>The third data block shows the shape of the requested ACM, as in the example.   /CORRIDOR/</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>2.;a.;(4);</stepDisplayId>
                <sortOrder>29</sortOrder>
                <stepTitle>Decode  SHAPE.  This dataset format was determined when the shape name was entered in field 3 of the ACMID dataset, as in the example;  /CORRIDOR/</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>2.;a.;(5);</stepDisplayId>
                <sortOrder>30</sortOrder>
                <stepTitle>Decode EFFLEVEL .  Vertical Dimension of the Effective Level Dataset defines the altitudes at which the ACM applies.                               EFFLEVEL/RARA:000AGL020AGL//                            EFFLEVEL/FLFL:FL250-FL290//</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>2.;a.;(6);</stepDisplayId>
                <sortOrder>31</sortOrder>
                <stepTitle>Decode APERIOD.  Airspace Time Period Dataset defines when the ACM is effective.                              APERIOD/INTERVAL/131325ZNOV/132359ZNOV/WEEKLY/4WK//</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>4</stepLevel>
                <stepDisplayId>2.;a.;(6);(a);</stepDisplayId>
                <sortOrder>32</sortOrder>
                <stepTitle>Field 1 defines the airspace time mode for the ACM.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>4</stepLevel>
                <stepDisplayId>2.;a.;(6);(b);</stepDisplayId>
                <sortOrder>33</sortOrder>
                <stepTitle>Field 2 defines the day, time and month the ACM starts.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>4</stepLevel>
                <stepDisplayId>2.;a.;(6);(c);</stepDisplayId>
                <sortOrder>34</sortOrder>
                <stepTitle>Field 3 defines the day, time and month the ACM stops.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>4</stepLevel>
                <stepDisplayId>2.;a.;(6);(d);</stepDisplayId>
                <sortOrder>35</sortOrder>
                <stepTitle>Field 4 defines the Interval Frequency and is used only if the airspace time mode is shown as &quot;Interval&quot;.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>4</stepLevel>
                <stepDisplayId>2.;a.;(6);(e);</stepDisplayId>
                <sortOrder>36</sortOrder>
                <stepTitle>Field 5 defines the Interval Duration and is used only if the airspace time mode is shown as &quot;Interval&quot;.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>3</stepLevel>
                <stepDisplayId>2.;a.;(7);</stepDisplayId>
                <sortOrder>37</sortOrder>
                <stepTitle>Decode CONTAUTH which is the Controlling Authority dataset.&#xD;
  CONTAUTH/RHEIN MAIN CP/125.25MHZ/341.25KHZ//</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>4</stepLevel>
                <stepDisplayId>2.;a.;(7);(a);</stepDisplayId>
                <sortOrder>38</sortOrder>
                <stepTitle>Field 1 identifies who will be responsible for the airspace coordinating measure.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>4</stepLevel>
                <stepDisplayId>2.;a.;(7);(b);</stepDisplayId>
                <sortOrder>39</sortOrder>
                <stepTitle>Field 2 will indicate the frequency to contact the controlling authority on.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>4</stepLevel>
                <stepDisplayId>2.;a.;(7);(c);</stepDisplayId>
                <sortOrder>40</sortOrder>
                <stepTitle>Additional data fields may be added to identify secondary and alternate frequencies.</stepTitle>
                <statements/>
            </childSteps>
            <childSteps isParentOrChild="Child">
                <stepLevel>2</stepLevel>
                <stepDisplayId>2.;b.;</stepDisplayId>
                <sortOrder>41</sortOrder>
                <stepTitle>Decode GEODATUM.  </stepTitle>
                <statements>
                    <note>This dataset is only used if the geodetic data standard used for the ACM is different than the standard cited in GEODATUM dataset in the ACO header.  If the same geodetic datum was used this field will not appear.</note>
                </statements>
            </childSteps>
            <childSteps isParentOrChild="Child">
